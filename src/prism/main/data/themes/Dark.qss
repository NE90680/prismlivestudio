    border: none;
}
QToolButton[For="SelectPathClear"]:hover {
    background-color: #222222;
}
QToolButton[For="SelectPathClear"]:pressed {
    background-color: #111111;
}


/* Spinbox and DoubleSpinbox */
QSpinBox, QDoubleSpinBox {
    background-color: #222222;
    border: none;
    border-radius: 3px;
    padding-left: 12px;
    min-height: 40px;
    max-height: 40px;
    font-size: 14px;
    color: #ffffff;
}
QSpinBox::up-button, QDoubleSpinBox::up-button {
    subcontrol-origin: margin;
    subcontrol-position: top right;

    background-color: #444444;
    width: 25px;
    border-top-right-radius: 3px;
}
QSpinBox::up-arrow, QDoubleSpinBox::up-arrow {
    image: url(./Dark/txt-dropbox-close-normal.png);
    width: 11px;
    height: 6px;
}
QSpinBox::up-button:hover, QDoubleSpinBox::up-button:hover {
    background-color: #555555;
}
QSpinBox::up-arrow:hover, QDoubleSpinBox::up-arrow:hover {
    image: url(./Dark/txt-dropbox-close-over.png);
    width: 11px;
    height: 6px;
}
QSpinBox::up-button:pressed, QDoubleSpinBox::up-button:pressed {
    background-color: #2d2d2d;
}
QSpinBox::up-arrow:pressed, QDoubleSpinBox::up-arrow:pressed {
    image: url(./Dark/txt-dropbox-close-click.png);
}
QSpinBox::up-arrow:disabled, QDoubleSpinBox::up-arrow:disabled {
    image: url(./Dark/txt-dropbox-close-disable.png);
}
QSpinBox::down-button, QDoubleSpinBox::down-button {
    subcontrol-origin: margin;
    subcontrol-position: bottom right;
    background-color: #444444;
    width: 25px;
    border-bottom-right-radius: 3px;
}
QSpinBox::down-arrow, QDoubleSpinBox::down-arrow {
    image: url(./Dark/txt-dropbox-open-normal.png);
    width: 11px;
    height: 6px;
}
QSpinBox::down-button:hover, QDoubleSpinBox::down-button:hover {
    background-color: #555555;
}
QSpinBox::down-arrow:hover, QDoubleSpinBox::down-arrow:hover {
    image: url(./Dark/txt-dropbox-open-over.png);
    width: 11px;
    height: 6px;
}
QSpinBox::down-button:pressed, QDoubleSpinBox::down-button:pressed {
    background-color: #2d2d2d;
}
QSpinBox::down-arrow:pressed, QDoubleSpinBox::down-arrow:pressed {
    image: url(./Dark/txt-dropbox-open-click.png);
}
QSpinBox::down-arrow:disabled, QDoubleSpinBox::down-arrow:disabled {
    image: url(./Dark/txt-dropbox-open-disable.png);
}

