/* Sliders */
QSlider::groove:horizontal {
    background-color: #1e1e1f;
    height: 4px;
    border: none;
}
QSlider::handle:horizontal {
	border-image: url(./Dark/img-handle-normal.png);
    height: 14px;
    width: 14px;
    margin: -5px 0px -5px 0px;
}

QSlider::handle:horizontal[handle="false"], QSlider::handle:horizontal[handle="false"][enter="false"]
, QSlider::handle:horizontal[handle="false"][enter="true"]{
    width: 0px;
}

QSlider::handle:horizontal[enter="false"], QSlider::handle:horizontal[enter="false"][handle="true"] {
	border-image: url(./Dark/img-handle-normal.png);
    height: 14px;
    width: 14px;
    margin: -5px 0px -5px 0px;
}

QSlider::handle:horizontal[enter="true"], QSlider::handle:horizontal[enter="true"][handle="true"] {
	border-image: url(./Dark/img-handle-over.png);
    height: 18px;
    width: 18px;
    margin: -7px 0px -7px 0px;
}

QSlider::sub-page:horizontal {
    background-color: #effc35;
}
QSlider::sub-page:horizontal:disabled {
    background-color: #2c2c2c;
}
QSlider::groove:vertical {
    background-color: #555555;
    width: 2px;
    border: none;
}
QSlider::handle:vertical {
    border-image: url(./Dark/SliderPanel/thumb.png);
    height: 8px;
    width: 8px;
    margin: 0 -3px;
}
QSlider::add-page:vertical {
    background-color: #ffffff;
}
QSlider::add-page:vertical:disabled {
    background-color: #2c2c2c;
}
QSlider::handle:disabled {
    border-image: none;
}


/* Table View */

QTableView {
    gridline-color: rgb(88,87,88); /* kindaDark */
}

QHeaderView::section {
    background-color: #333333;
    color: rgb(225,224,225); /* veryLight */
