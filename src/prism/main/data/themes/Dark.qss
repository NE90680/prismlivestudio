/* Custom theme information.  This will set the application's QPalette, as
 * well as pass to QML via the PLSTheme object.
 * Can also use PLSTheme::disabled, PLSTheme::active, and PLSTheme::inactive.
 * Using it without will set all three (making 'active' a bit redundant) */

QWidget:disabled {
    color: #666666;
}

QWidget {
    color: #ffffff;
    font-family: "Segoe UI","MalgunGothic","Malgun Gothic","Dotum","Gulim";
}

* [frameShape="1"], * [frameShape="2"], * [frameShape="3"], * [frameShape="4"], * [frameShape="5"], * [frameShape="6"] {
    border: 1px solid #111111;
}

QToolTip {
    background-color: #111111;
    border: none;
    padding: 5px;
    font-size: 11px;
    color: #bababa;
}

QMenu {
    font-size: 14px; /* Warning: here must be defined and should be same with QMenu::item */
    border: 1px solid #111111;
    background: #2d2d2d;
	padding-top: 8px;
	padding-bottom: 8px;
	padding-left: 0px;
	padding-right: 0px;
}
QMenu::right-arrow {
    padding: 0px 10px 0px 0px;
    image: url(./Dark/btn-txtlayer-close-normal.png);
}
QMenu::icon {
    padding-left: 10px;
    min-width: 50px;
    max-width: 50px;
    min-height: 30px;
    max-height: 30px;
}
QMenu::item {
    font-size: 14px;
    padding: 0px 20px 0px 20px;
    min-height: 40px;
    max-height: 40px;
	min-width: 190px;
    color: #ffffff;
    background: #2d2d2d;
}
QMenu::item:selected { /* apply to mouse hover */
    background: #373737;
}
QMenu::item:checked{
    color: #effc35;
}
QMenu::item::!enabled{
	color: #666666;
}
QMenu::separator {
    height: 1px;
    background: #111111;
}
QMenu::indicator{
	border:none;
}

PLSBasicSourceSelect QListWidget,
PLSBasicSourceSelect QLineEdit,
PLSBasicSourceSelect QCheckBox,
PLSBasicProperties QRadioButton {
    font-size: 14px;
    font-weight: normal;
}

PLSBasicSourceSelect QRadioButton,
PLSPropertiesView QLabel { /* Here cann't use PLSBasicProperties to avoid conflict with alert window */
	font-size: 14px;
    font-weight: bold;
}

PLSBasicFilters PLSPopupMenu#basicFilterMenu{
	min-width: 180px;
	max-width: 180px;
}

PLSBasicFilters PLSPopupMenu#basicFilterMenu PLSPopupMenu{
	min-width: 260px;
	max-width: 260px;
}

PLSBasicFilters PLSPopupMenu#basicFilterMenu QLabel{
	background-color: transparent;
}

PLSBasicFilters PLSPopupMenu#basicFilterMenu PLSPopupMenuItem[selected="true"] {
    background-color: #444444;
}

PLSBasic #sceneMenu, PLSBasic #sceneMenu QMenu{
	padding-top: 8px;
	padding-bottom: 8px;
    background-color: #2d2d2d;
    border: 1px solid #111111;
	color:#ffffff;
	font-size: 14px;
}

PLSBasic #sceneMenu::item:selected ,PLSBasic #sceneMenu QMenu::selected{
    background-color: #373737;
    color: #ffffff;
}

PLSBasic #sceneMenu::item::!enabled{
	color: #666666;
}

SourceTree{
	outline: none;
}

SourceTree:focus {
    outline: none;
}

QListWidget::item,SourceTree::item{
	height: 40px;
	outline: none;
	border:none;
}

SourceTreeItem,SourceTreeItem[status="normal"] {
	background-color: #272727;
}

SourceTreeItem[status="hover"] {
	background-color: #444444;
}

SourceTreeItem[status="pressed"] {
	background-color: #2d2d2d;
}

PLSBasicProperties QLineEdit,
QListWidget QLineEdit,
SourceTree QLineEdit {
    padding-top: 0px;
    padding-bottom: 0px;
    padding-right: 0;
    padding-left: 2px;
    border: none;
    border-radius: none;
	min-height: 20px;
	max-height: 20px;
	background-color: #1e1e1e;
    font-size: 14px;
}

PLSSceneItemView QLineEdit,
PLSFiltersItemView QLineEdit,
PLSSceneTransitionsView QLineEdit {
    font-size: 14px;
}

/* Dock Widget */
PLSDockTitle {
    qproperty-captionHeight: 40;
    qproperty-marginLeft: 14;
    qproperty-marginRight: 14;
    qproperty-contentSpacing: 2;
    background-color: #272727;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
}
PLSDockTitle #titleLabel {
    font-size: 14px;
    font-weight: bold;
    color: #ffffff;
}
PLSDockTitle #advButton {
	background: transparent;
    image: url(./Dark/icon-more-default.png);
}

PLSDockTitle #advButton:hover{
    border-image:url("./Dark/icon-more-over.png");
}

PLSDockTitle #advButton:pressed{
    border-image:url("./Dark/icon-more-click.png");
}

PLSDockTitle #advButton:!enabled{
    border-image:url("./Dark/icon-more-disable.png");
}


PLSDockTitle QToolButton {
    border: none;
    border-image: none;
    background-color: #272727;
    min-width: 22px;
    max-width: 22px;
    min-height: 22px;
    max-height: 22px;
}
PLSDockTitle QToolButton:hover {
    border-image: none;
    background-color: #363636;
    border-radius: 3px;
}
PLSDockTitle QToolButton:pressed {
    border-image: none;
    background-color: #111111;
}
PLSDock {
    qproperty-contentMarginLeft: 15;
    qproperty-contentMarginRight: 15;
    qproperty-contentMarginBottom: 15;
    border: 1px solid #111111;
    background-color: #1e1e1f;
}
PLSDock #content {
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
}
PLSDock .QWidget,
PLSDock .QToolBar,
PLSDock .QScrollArea,
PLSDock .QFrame,
PLSDock .QLabel,
PLSDock QListWidget,
PLSDock QListView,
PLSDock QToolBar .QToolButton {
    background-color: #272727;
    border: none;
    border-radius: 0;
}
PLSDock QToolButton {
    min-width: 22px;
    max-width: 22px;
    min-height: 22px;
    max-height: 22px;
}
PLSDock QToolButton:hover {
    background-color: #363636;
}
PLSDock QToolButton:pressed {
    background-color: #111111;
}
PLSDock[moving="true"] PLSDockTitle {
    border-left: 1px solid #effc35;
    border-top: 1px solid #effc35;
    border-right: 1px solid #effc35;
}
PLSDock[moving="true"] #content {
    border-left: 1px solid #effc35;
    border-right: 1px solid #effc35;
    border-bottom: 1px solid #effc35;
}
PLSDock[movingAndFloating="true"] {
    border: 1px solid #effc35;
}
PLSDock[movingAndFloating="true"] PLSDockTitle, PLSDock[movingAndFloating="true"] #content {
    border: none;
}
PLSDock[floating="true"] {
    background-color: #272727;
}
PLSDock QFrame[frameShape="1"],
PLSDock QFrame[frameShape="2"],
PLSDock QFrame[frameShape="3"],
PLSDock QFrame[frameShape="4"],
PLSDock QFrame[frameShape="5"],
PLSDock QFrame[frameShape="6"] {
    border: none;
}

/* Group Box */
QGroupBox {
    border: none;
    padding-top: 27px;
    font-size: 20px;
    font-weight: bold;
    color: #ffffff;
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: -4px;
    top: 1px;
}
QGroupBox[useFor="GroupBoxLevel2"] {
    border: none;
    padding-top: 17px;
    font-size: 14px;
    font-weight: bold;
    color: #ffffff;
}
QGroupBox[useFor="GroupBoxLevel2"]::title {
    subcontrol-origin: margin;
    left: -2px;
    top: 1px;
}
QLabel[useFor="QGroupBox"] {
    font-size: 20px;
    font-weight: bold;
    color: #ffffff;
}

/* ScrollBars */

::corner {
    background-color: #272727;
    border: none;
}

QScrollBar:vertical {
    background-color: #272727;
    width: 10px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background-color: #444444;
    min-height: 20px;
    border-radius: 3px;
    border: none;
    margin: 0 2px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical, QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    border: none;
    background: none;
    color: none;
}

QScrollBar:horizontal {
    background-color: #272727;
    height: 10px;
    margin: 0px;
}

QScrollBar::handle:horizontal {
    background-color: #444444;
    min-width: 20px;
    border-radius: 3px;
    border: none;
    margin: 2px 0;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none;
    background: none;
    width: 0px;
}

QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal, QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    border: none;
    background: none;
    color: none;
}


* [themeID="addIconSmall"] {
    qproperty-icon: url(./Dark/icon-addsource-default.png);
}

* [themeID="removeIconSmall"] {
    qproperty-icon: url(./Dark/ic-delete-default.png);
}

* [themeID="propertiesIconSmall"] {
    qproperty-icon: url(./Dark/settings/ic-setting-s-normal.png);
}

* [themeID="configIconSmall"] {
    qproperty-icon: url(./Dark/settings/general.svg);
}

* [themeID="refreshIconSmall"] {
    qproperty-icon: url(./Dark/refresh.svg);
}

* [themeID="upArrowIconSmall"] {
    qproperty-icon: url(./Dark/ic-top-normal.png);
}

* [themeID="downArrowIconSmall"] {
    qproperty-icon: url(./Dark/ic-down-normal.png);
}

* [themeID="pauseIconSmall"] {
    qproperty-icon: url(./Dark/media-pause.svg);
}

* [borders="top-only"] {
    border: none;
    border-top: 1px solid #111111;
}


/* Tab Widget */

QTabWidget::pane { /* The tab widget frame */
    border-top: 1px solid rgb(31,30,31); /* veryDark */
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar::tab {
    background-color: rgb(76,76,76);
    border: none;
    padding: 5px;
    min-width: 50px;
    margin: 1px;
}

QTabBar::tab:top {
    border-bottom: 1px transparent;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;

}

QTabBar::tab:bottom {
    padding-top: 1px;
    margin-bottom: 4px;
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
    height: 14px;
}

QTabBar::tab:selected {
    background-color: rgb(31,30,31); /* veryDark */
}

QTabBar::tab:hover {
    background-color: rgb(122,121,122); /* light */
}

QTabBar::tab:pressed {
    background-color: rgb(31,30,31); /* veryDark */
}

QFontDialog QFontListView {
    show-decoration-selected: 1;
    background-color: #222222;
    font-size: 14px;
    color: #ffffff;
    border: 1px solid #111111;
    border-radius: 3px;
}
QFontDialog QGroupBox {
    border: 1px solid #111111;
    padding-top: 20px;
    font-size: 14px;
    font-weight: bold;
    color: #ffffff;
}
QFontDialog QGroupBox::title {
    subcontrol-origin: margin;
    left: 4px;
    top: 2px;
}
QFontDialog QGroupBox QLineEdit {
   max-width: 10000px;
   max-height: 10000px;
}

QColorDialog QColorLuminancePicker {
   background-color: #2c2c2c;
}

/* QCheckBox */
QCheckBox {
    outline: none;
}
QCheckBox::indicator:checked {
    image: url(./Dark/check-box-normal.png);
}
QCheckBox::indicator:checked:hover {
    image: url(./Dark/check-box-over.png);
}
QCheckBox::indicator:checked:pressed {
    image: url(./Dark/check-box-clicked.png);
}
QCheckBox::indicator:checked:disabled {
    image: url(./Dark/check-box-disable.png);
}
QCheckBox::indicator:unchecked {
    image: url(./Dark/check-box-uncheked-normal.png);
}
QCheckBox::indicator:unchecked:hover {
    image: url(./Dark/check-box-uncheked-over.png);
}
QCheckBox::indicator:unchecked:disabled {
    image: url(./Dark/check-box-uncheked-disable.png);
}

/* QRadioButton */
QRadioButton {
    outline: none;
}
QRadioButton::indicator:checked {
    image: url(./Dark/radio-nutton-on.png);
}
QRadioButton::indicator:checked:disabled {
    image: url(./Dark/radio-nutton-on-disable.png);
}
QRadioButton::indicator:unchecked {
    image: url(./Dark/radio-nutton-off.png);
}

/* Textedits etc */
QPlainTextEdit{
	border: none;
	border-radius: 3px;
	background-color: #222222;
	padding: 2px 2px 3px 7px;
	font-size: 14px;
	color: #ffffff;
}

QTextEdit {
    background-color: #2c2c2c;
    border: none;
    padding: 2px 2px 3px 7px;
}
QLineEdit {
    background-color: #222222;
    border: none;
    border-radius: 3px;
    padding-left: 12px;
    min-height: 40px;
    max-height: 40px;
}
QLineEdit[For="SelectPath"] {
    background-color: #222222;
    border: none;
    border-radius: 3px;
    padding-left: 12px;
    min-height: 40px;
    max-height: 40px;
}

/* QToolButton */
QToolButton[For="SelectPath"] {
    border-image: url(./Dark/browser-normal.png);
    min-width: 40px;
    max-width: 40px;
    min-height: 40px;
    max-height: 40px;
    border-radius: 3px;
}
QToolButton[For="SelectPath"]:hover {
    border-image: url(./Dark/browser-over.png);
}
QToolButton[For="SelectPath"]:pressed {
    border-image: url(./Dark/browser-clicked.png);
}
QToolButton[For="SelectPath"]:disabled {
    border-image: url(./Dark/browser-disable.png);
}

QToolButton[For="SelectPathClear"] {
    background-color: #444444;
    min-width: 40px;
    max-width: 40px;
    min-height: 40px;
